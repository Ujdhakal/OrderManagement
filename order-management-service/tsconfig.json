{
    "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "module": "commonjs",
      "target": "ESNext",
      "strict": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "esModuleInterop": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "incremental": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  